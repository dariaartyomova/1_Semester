#include <iostream>

int main()
{
    int n;
    int m;
    int minsum = 100000;
    std::cin >> n;
    std::cin >> m;
    int matrix[100][100];


    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            std::cin >> matrix[i][j];

    for (int i = 0; i < n; i++)
    {
        int sum = 0;
        for (int j = 0; j < n; j++)
        {
           
            sum += matrix[i][j];
        }
        if(sum < minsum)
        {
            minsum = sum;
        }
    }
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            matrix[i][j] = minsum;
    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < n; j++)
            std::cout << matrix[i][j] << " ";
        std::cout << std::endl;

    }
        
    return 0;
        
    
}
